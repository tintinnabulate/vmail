<html>
  <head>
    <title></title>
  </head>
  <body>
    <form id="register" action="/register" method="post">
      <div class="form-group">
        <label for="First_Name" class="col-sm-4 control-label">First Name * :</label>
        <div class="col-sm-8">
          <input type="text" class="form-control required" id="First_Name" name="First_Name" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="Last_Name" class="col-sm-4 control-label">Last Name * :</label>
        <div class="col-sm-8">
          <input type="text" class="form-control required" id="Last_Name" name="Last_Name" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="Email_Address" class="col-sm-4 control-label">Email Address * :</label>
        <div class="col-sm-8">
          <input type="Email_Address" class="form-control required" id="Email_Address" name="Email_Address" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="Password" class="col-sm-4 control-label">Password * :</label>
        <div class="col-sm-8">
          <input type="Password" class="form-control required" id="Password" name="Password">
        </div>
      </div>
      <div class="form-group">
        <label for="Conf_Password" class="col-sm-4 control-label">Confirm Password * :</label>
        <div class="col-sm-8">
          <input type="Password" class="form-control required" id="Conf_Password" name="Conf_Password">
        </div>
      </div>
      <div class="form-group">
        <label for="The_Country" class="col-sm-4 control-label">Country :</label>
        <div class="col-sm-4">
          <select name="The_Country" id="The_Country" class="form-control medium valid">
            {{ range $i, $country := $.Countries }}
            <option value="{{ $i }}">{{ $country }}</option>
            {{ end }}
          </select>
        </div>
      </div>
      <div class="form-group">
        <label for="Zip_or_Postal_Code" class="col-sm-4 control-label">Zip/Postal Code * :</label>
        <div class="col-sm-8">
          <input type="text" class="form-control required" id="Zip_or_Postal_Code" name="Zip_or_Postal_Code" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="City" class="col-sm-4 control-label">City * :</label>
        <div class="col-sm-8">
          <input type="text" class="form-control required" name="City" id="City" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="State" class="col-sm-4 control-label">State * :</label>
        <div class="col-sm-8">
          <input type="text" class="form-control required" name="State" id="State" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="Phone_Number" class="col-sm-4 control-label">Phone Number :</label>
        <div class="col-sm-8">
          <input type="text" class="form-control" id="Phone_Number" name="Phone_Number" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="Sobriety_Date" class="col-sm-4 control-label">
        Sobriety Date :
        <span class="tiny">(mm/dd/yyyy)</span>
        </label>
        <div class="col-sm-4">
          <input type="text" class="form-control datepickerMasked valid" id="Sobriety_Date" name="Sobriety_Date" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="Birth_Date" class="col-sm-4 control-label">
        Birth Date :
        <span class="tiny">(mm/dd/yyyy)</span>
        </label>
        <div class="col-sm-4">
          <input type="text" class="form-control datepickerMasked2" id="Birth_Date" name="Birth_Date" value="">
        </div>
      </div>
      <div class="form-group">
        <label for="Member_Of" class="col-sm-4 control-label">Member Of :</label>
        <div class="col-sm-8">
          {{ range $i, $fellowship := $.Fellowships }}
          <div class="checkbox">
            <label><input type="checkbox" name="Member_Of" value="{{ $i }}">{{ $fellowship }}</label>
          </div>
          {{ end }}
        </div>
        <div class="form-group">
          <label for="YPAA_Committee" class="col-sm-4 control-label">YPAA Committee :</label>
          <div class="col-sm-8">
            <input type="text" class="form-control" id="YPAA_Committee" name="YPAA_Committee" value="">
          </div>
        </div>
        <div class="form-group">
          <label for="Any_Special_Needs" class="col-sm-4 control-label">Special Needs :</label>
          <div class="col-sm-8">
            {{ range $i, $specialneed := $.SpecialNeeds }}
            <div class="checkbox">
              <label><input type="checkbox" name="Any_Special_Needs" value="{{ $i }}">{{ $specialneed }}</label>
            </div>
            {{ end }}
          </div>
        </div>
        <div class="form-group">
          <label for="service" class="col-sm-4 control-label"> Service Opportunities :</label>
          <div class="col-sm-8">
            {{ range $i, $opportunity := $.ServiceOpportunities }}
            <div class="checkbox">
              <label><input type="checkbox" name="Any_Service_Opportunities" value="{{ $i }}">{{ $opportunity }}</label>
            </div>
            {{ end }}
          </div>
        </div>
        <div class="form-group">
          <label for="Comments" class="col-sm-4 control-label">Comments :</label>
          <div class="col-sm-8">
            <textarea name="Comments" class="form-control" rows="3"></textarea>
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-12">
            {{ .csrfField }}
            <input type="submit" value="Next Step â€º" class="btn btn-primary">
          </div>
        </div>
      </div>
    </form>
  </body>
</html>
